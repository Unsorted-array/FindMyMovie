<h1>Result Page</h1>
<ul>
   
<%data['Search'].forEach(function(movie) {%>
   <li><strong> <%=  movie['Title'] %></strong></li>
    <li> <em><%=  movie['Year'] %></em></li>
    <li>  <img src="<%=  movie['Poster'] %>"></li>
<%})%>
</ul>

<form action="#" method="GET">
    <input type="text" id="movieName" placeholder="Movie Name" name="search" >
    <input type="submit" >
</form>
